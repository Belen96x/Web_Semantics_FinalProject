#    Object Properties
#################################################################

###  http://example.org/ufo#hasShape
ex:hasShape rdf:type owl:ObjectProperty ;
            rdfs:domain ex:UFO ;
            rdfs:range ex:UfoShape ;
            rdfs:comment "UFO hasShape UFOShape" .


###  http://example.org/ufo#partOfCountry
ex:partOfCountry rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain ex:State ;
                 rdfs:range ex:Country ;
                 owl:propertyDisjointWith ex:partOfState ;
                 rdfs:comment "State PartOfCountry Country" .


###  http://example.org/ufo#partOfState
ex:partOfState rdf:type owl:ObjectProperty ;
               rdfs:domain ex:CityArea ;
               rdfs:range ex:State .


###  http://example.org/ufo#wasSeen
ex:wasSeen rdf:type owl:ObjectProperty ;
           rdfs:domain ex:UFO ;
           rdfs:range ex:latitude ,
                      ex:longitude ;
           rdfs:comment "UFO wasSeen at Latitude and Longitude" .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:FunctionalProperty .


