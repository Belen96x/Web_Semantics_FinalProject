<http://www.semanticweb.org/belen/ontologies/2024/5/ufo> rdf:type owl:Ontology ;
                                                          rdfs:comment "This is an ontology for UFO Sightings as part of the final project for the Web Semantics M1 TAL UL"@en .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo#partOfCountry
:partOfCountry rdf:type owl:ObjectProperty ;
               rdfs:domain :state ;
               rdfs:range :country .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo#partOfTownVillage
:partOfTownVillage rdf:type owl:ObjectProperty ;
                   rdfs:domain :townVillage ;
                   rdfs:range :country .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/hasDuration
:hasDuration rdf:type owl:ObjectProperty ;
             rdfs:domain :ufoSighting ;
             rdfs:range :sightingDuration .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/hasOccupation
:hasOccupation rdf:type owl:ObjectProperty ;
               rdfs:domain :witness ;
               rdfs:range :occupationWitness ;
               rdfs:comment "witness hasOcupation occupationWitness"@en .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/hasShape
:hasShape rdf:type owl:ObjectProperty ;
          rdfs:domain :ufo ;
          rdfs:range :ufoShape ;
          rdfs:comment "ufo hasShape ufoShape" .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/partOfState
:partOfState rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :cityArea ;
             rdfs:range :state ;
             rdfs:comment "cityArea partOfState state" .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/wasDocumented
:wasDocumented rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :ufoSighting .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/wasSeen
:wasSeen rdf:type owl:ObjectProperty ;
         rdfs:domain :ufo ;
         rdfs:range :witness ;
         rdfs:comment "ufo wasSeen longitude and latitude and dateSightnign by witness" .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdfs:domain :cityArea ;
                      rdfs:range :state .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo#dateTimeDocumented
:dateTimeDocumented rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :ufoSighting ;
                    rdfs:range xsd:dateTime ;
                    owl:propertyDisjointWith :dateTimeSighting .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo#dateTimeSighting
:dateTimeSighting rdf:type owl:DatatypeProperty ;
                  rdfs:domain :ufoSighting ;
                  rdfs:range xsd:dateTime .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/latitude
:latitude rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :cityArea ;
          rdfs:range xsd:float .


###  http://www.semanticweb.org/belen/ontologies/2024/5/ufo/longitude
:longitude rdf:type owl:DatatypeProperty ;
           rdfs:domain :cityArea ;
           rdfs:range xsd:float .


