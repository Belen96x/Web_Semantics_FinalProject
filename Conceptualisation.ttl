@prefix : <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Define Classes
:Sighting a owl:Class .
:Location a owl:Class .
:UFO a owl:Class .

# Define Properties
:date_time a owl:DatatypeProperty ;
    rdfs:domain :Sighting ;
    rdfs:range xsd:dateTime .

:encounter_length a owl:DatatypeProperty ;
    rdfs:domain :Sighting ;
    rdfs:range xsd:double .

# Define Relationships
:occursAt a owl:ObjectProperty ;
    rdfs:domain :Sighting ;
    rdfs:range :Location .

:involves a owl:ObjectProperty ;
    rdfs:domain :Sighting ;
    rdfs:range :UFO .

# Sample Data
:Sighting1 a :Sighting ;
    :date_time "2024-05-31T20:00:00"^^xsd:dateTime ;
    :encounter_length 300.0 ;
    :occursAt :Location1 ;
    :involves :UFO1 .

:Location1 a :Location ;
    :city_area "New York" ;
    :state "NY" ;
    :country "USA" ;
    :latitude 40.7128 ;
    :longitude -74.0060 .

:UFO1 a :UFO ;
    :ufo_shape "Disc" .
